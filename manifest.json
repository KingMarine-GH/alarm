{
    "manifest_version": 3,
    "name": "Alarm by KingMarine",
    "version": "0.0.1",
    "background": {
        "service_worker": "js/background.js"
    },
    "permissions": [
        "storage",
        "notifications",
        "alarms"
    ],
    "action": {
        "default_popup": "html/popup.html"
    }
}